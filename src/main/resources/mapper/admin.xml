<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.tenco.hobby.repository.interfaces.AdminRepository">
	
	<select id="findAllQandA" resultType="com.tenco.hobby.repository.model.QandA">
		select * from q_a where proceed = 0;
	</select>
	
	<select id="findbyIdQuestion" resultType="com.tenco.hobby.repository.model.QandA">
		select * from q_a where id = #{id};
	</select>
</mapper>