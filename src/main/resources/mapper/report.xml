<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.tenco.hobby.repository.interfaces.ReportRepository">

	<insert id="insertReportBoard">
		insert into report_board_tb(report_user_id, report_board_id)
		values( #{userId} , #{boardId})
	</insert>
	
	<insert id="insertReportComment">
		insert into report_comment_tb(report_user_id, report_comment_id)
		values( #{userId} , #{commentId})
	</insert>




</mapper>